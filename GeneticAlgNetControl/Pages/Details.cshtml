<div class="row h4 mt-3">
    <div class="col-1 text-center">
        <span><i class="fas fa-share-alt"></i></span>
    </div>
    <div class="col-11 text-left">
        <span>Network</span>
    </div>
</div>
<hr />
<div class="row">
    <div class="offset-1 col-11">
        <div class="row">
            <div class="col-lg-6 order-1 order-lg-1">
                <label asp-for="View.Algorithm.Nodes" class="font-weight-bold">Nodes</label>
            </div>
            <div class="col-lg-6 order-1 order-lg-2 mb-3">
                <div class="input-group modal-group">
                    <input asp-for="View.Algorithm.Nodes" class="form-control form-control-plaintext" value="@Model.View.Algorithm.Nodes.Count()" disabled />
                    <div class="input-group-append">
                        <button class="btn btn-dark" type="button" data-toggle="modal" data-target=".details-modal">
                            <span>View</span>
                        </button>
                    </div>
                    <div class="modal-group-data" hidden>
                        <div class="modal-group-title">
                            <span class="font-weight-bold">Nodes</span>
                        </div>
                        <div class="modal-group-body">
                            <ul class="list-group list-group-flush">
                                <li class="list-group-item list-group-item-dark list-group-item-action active">
                                    <span class="font-weight-bold">Node</span>
                                </li>
                                @foreach (var item in Model.View.Algorithm.Nodes)
                                {
                                    <li class="list-group-item list-group-item-action">
                                        <span>@item</span>
                                    </li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 order-1 order-lg-1">
                <label asp-for="View.Algorithm.Edges" class="font-weight-bold">Edges</label>
            </div>
            <div class="col-lg-6 order-1 order-lg-2 mb-3">
                <div class="input-group modal-group">
                    <input asp-for="View.Algorithm.Edges" class="form-control form-control-plaintext" value="@Model.View.Algorithm.Edges.Count()" disabled />
                    <div class="input-group-append">
                        <button class="btn btn-dark" type="button" data-toggle="modal" data-target=".details-modal">
                            <span>View</span>
                        </button>
                    </div>
                    <div class="modal-group-data" hidden>
                        <div class="modal-group-title">
                            <span class="font-weight-bold">Edges</span>
                        </div>
                        <div class="modal-group-body">
                            <ul class="list-group list-group-flush">
                                <li class="list-group-item list-group-item-dark list-group-item-action active">
                                    <div class="row">
                                        <div class="col-6">
                                            <span class="font-weight-bold">Source node</span>
                                        </div>
                                        <div class="col-6">
                                            <span class="font-weight-bold">Target node</span>
                                        </div>
                                    </div>
                                </li>
                                @foreach (var item in Model.View.Algorithm.Edges)
                                {
                                    <li class="list-group-item list-group-item-action">
                                        <div class="row">
                                            <div class="col-6">
                                                <span>@item.SourceNode</span>
                                            </div>
                                            <div class="col-6">
                                                <span>@item.TargetNode</span>
                                            </div>
                                        </div>
                                    </li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-6 order-1 order-lg-1">
                <label asp-for="View.Algorithm.TargetNodes" class="font-weight-bold">Target nodes</label>
            </div>
            <div class="col-lg-6 order-1 order-lg-2 mb-3">
                <div class="input-group modal-group">
                    <input asp-for="View.Algorithm.TargetNodes" class="form-control form-control-plaintext" value="@Model.View.Algorithm.TargetNodes.Count()" disabled />
                    <div class="input-group-append">
                        <button class="btn btn-dark" type="button" data-toggle="modal" data-target=".details-modal">
                            <span>View</span>
                        </button>
                    </div>
                    <div class="modal-group-data" hidden>
                        <div class="modal-group-title">
                            <span class="font-weight-bold">Target nodes</span>
                        </div>
                        <div class="modal-group-body">
                            <ul class="list-group list-group-flush">
                                <li class="list-group-item list-group-item-dark list-group-item-action active">
                                    <span class="font-weight-bold">Node</span>
                                </li>
                                @foreach (var item in Model.View.Algorithm.TargetNodes)
                                {
                                    <li class="list-group-item list-group-item-action">
                                        <span>@item</span>
                                    </li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 order-1 order-lg-1">
                <label asp-for="View.Algorithm.PreferredNodes" class="font-weight-bold">Preferred nodes</label>
            </div>
            <div class="col-lg-6 order-1 order-lg-2 mb-3">
                <div class="input-group modal-group">
                    <input asp-for="View.Algorithm.PreferredNodes" class="form-control form-control-plaintext" value="@Model.View.Algorithm.PreferredNodes.Count()" disabled />
                    <div class="input-group-append">
                        <button class="btn btn-dark" type="button" data-toggle="modal" data-target=".details-modal">
                            <span>View</span>
                        </button>
                    </div>
                    <div class="modal-group-data" hidden>
                        <div class="modal-group-title">
                            <span class="font-weight-bold">Preferred nodes</span>
                        </div>
                        <div class="modal-group-body">
                            <ul class="list-group list-group-flush">
                                <li class="list-group-item list-group-item-dark list-group-item-action active">
                                    <span class="font-weight-bold">Node</span>
                                </li>
                                @foreach (var item in Model.View.Algorithm.PreferredNodes)
                                {
                                    <li class="list-group-item list-group-item-action">
                                        <span>@item</span>
                                    </li>
                                }
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
